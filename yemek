public class Yemek extends Urun {
    private String tur;

    public Yemek(String ad, double fiyat, String tur) {
        super(ad, fiyat);
        this.tur = tur;
    }

    public String getTur() {
        return tur;
    }

    @Override
    public String toString() {
        return super.toString() + ", TÃ¼r: " + tur;
    }}
